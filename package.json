    "postcss-cli": "^7.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.2",
    "raw-loader": "^4.0.0",
    "rc-color-picker": "^1.2.6",
    "react": "^16.13.1",
    "react-app-rewire-postcss": "^3.0.2",
    "react-app-rewired": "^2.1.5",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-monaco-editor": "^0.35.0",
    "react-refresh": "^0.8.1",
    "react-rnd": "^10.1.8",
    "react-scripts": "3.4.1",
    "react-zoom-pan-pinch": "^1.6.1",
    "recast": "^0.18.8",
    "rgb-hex": "^3.0.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "svg-url-loader": "^5.0.0",
    "tailwindcss": "^1.2.0",
    "typescript": "~3.7.2",
    "unionfs": "^4.4.0",
    "webpack": "4.42.0",
    "worker-loader": "^2.0.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "electron": "electron .",
    "postinstall": "patch-package && npx electron-rebuild -o node-sass,oniguruma",
    "lint": "npx eslint --ext .ts,.tsx,.js .",
    "lint:fix": "npx eslint --ext .ts,.tsx,.js --fix .",
    "pretty": "npx prettier --write .",
    "typecheck": "npx tsc --noEmit"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "simple-import-sort",
      "import"
    ],
    "rules": {
      "simple-import-sort/sort": [
        "error",
        {
          "groups": [
            [
              "^react",
              "^@?\\w"
            ],
            [
              "^\\u0000"
            ],
            [
              "^\\.\\.(?!/?$)",
              "^\\.\\./?$",
              "^\\./(?=.*/)(?!/?$)",
              "^\\.(?!/?$)",
              "^\\./?$",
              "^.+\\.s?css$"
            ]
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.3.0-beta.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/babel__standalone": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.149",
    "@types/memory-fs": "^0.3.2",
    "@types/node": "^12.0.0",
    "@types/prettier": "^2.0.0",
    "@types/react": "^16.9.0",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^16.9.0",
    "@types/webpack": "^4.41.10",
    "electron-rebuild": "^1.10.1",
    "eslint": "^6.8.0",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "node-gyp": "^6.1.0"
  }
}
