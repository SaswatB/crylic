  "scripts": {
    "start": "node ./scripts/start",
    "build": "npx rimraf app/build && npx rimraf app/build-main && webpack --config config/webpack.main.config.js --mode production && node --max-old-space-size=8192 ./scripts/build && npx cpy --parents build-main app/ && npx cpy --parents build app/ && npx rimraf --glob ./app/build*/**/*.map && node ./scripts/postbuild.js",
    "test": "node ./scripts/test",
    "electron": "webpack --config config/webpack.main.config.js --mode development && electron --remote-debugging-port=9222 .",
    "package": "electron-builder",
    "postinstall": "patch-package",
    "lint": "npx eslint --ext .ts,.tsx,.js src/.",
    "lint:fix": "npx eslint --ext .ts,.tsx,.js --fix src/.",
    "pretty": "npx prettier --write .",
    "typecheck": "npx tsc --noEmit"
  },
