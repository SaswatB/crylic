{
  "name": "paint",
  "version": "0.1.0",
  "private": true,
  "author": "Saswat Bhattacharya",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "npx rimraf app/build && react-app-rewired build && npx node-mv build app/build",
    "test": "jest test",
    "eject": "react-app-rewired eject",
    "electron": "electron .",
    "package": "electron-builder",
    "postinstall": "patch-package && npx electron-rebuild -o node-sass,oniguruma",
    "lint": "npx eslint --ext .ts,.tsx,.js src/.",
    "lint:fix": "npx eslint --ext .ts,.tsx,.js --fix src/.",
    "pretty": "npx prettier --write .",
    "typecheck": "npx tsc --noEmit"
  },
  "dependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/standalone": "^7.9.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@material-ui/core": "^4.9.9",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.51",
    "autoprefixer": "^9.7.5",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "customize-cra": "^0.9.1",
    "deep-freeze-strict": "^1.1.1",
    "dotenv": "^8.2.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-is-dev": "^1.2.0",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "fp-ts": "^2.5.3",
    "gonzales-pe": "^4.3.0",
    "immer": "^6.0.3",
    "io-ts": "^2.2.1",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "lodash": "^4.17.15",
    "material-ui-popup-state": "^1.5.4",
    "memfs": "^3.2.0",
    "memory-fs": "^0.5.0",
    "monaco-editor-textmate": "^2.2.1",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "monaco-textmate": "^3.0.1",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "notistack": "^0.9.9",
    "oniguruma": "^7.2.1",
    "patch-package": "^6.2.2",
    "postcss": "^7.0.31",
    "postcss-cli": "^7.1.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.2",
    "raw-loader": "^4.0.0",
    "rc-color-picker": "^1.2.6",
    "re-resizable": "^6.3.2",
    "react": "^16.13.1",
    "react-app-rewire-postcss": "^3.0.2",
    "react-app-rewired": "^2.1.5",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-draggable": "^4.3.1",
    "react-hotkeys-hook": "^2.1.3",
    "react-monaco-editor": "^0.35.0",
    "react-refresh": "^0.8.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-zoom-pan-pinch": "^1.6.1",
    "recast": "^0.18.8",
    "rgb-hex": "^3.0.0",
    "rxjs": "^6.5.5",
    "sass-loader": "^8.0.2",
    "sass.js": "^0.11.1",
    "sassjs-loader": "^2.0.0",
    "send": "^0.17.1",
    "style-loader": "^1.2.1",
    "tailwindcss": "^1.4.6",
    "typescript": "^4.0.0-dev.20200501",
    "unionfs": "^4.4.0",
    "webpack": "4.42.0",
    "worker-loader": "^2.0.0",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.3.0-beta.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/babel__standalone": "^7.1.2",
    "@types/express": "^4.17.6",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.149",
    "@types/memory-fs": "^0.3.2",
    "@types/node": "^12.0.0",
    "@types/node-sass": "^4.11.0",
    "@types/prettier": "^2.0.0",
    "@types/react": "^16.9.0",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack": "^4.41.10",
    "@types/yauzl": "^2.9.1",
    "electron": "^8.2.0",
    "electron-builder": "^22.7.0",
    "electron-rebuild": "^1.10.1",
    "eslint": "^6.8.0",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "jest-raw-loader": "^1.0.1",
    "node-gyp": "^6.1.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "simple-import-sort",
      "import"
    ],
    "rules": {
      "simple-import-sort/sort": [
        "error",
        {
          "groups": [
            [
              "^react",
              "^@?\\w"
            ],
            [
              "^\\u0000"
            ],
            [
              "^\\.\\.(?!/?$)",
              "^\\.\\./?$",
              "^\\./(?=.*/)(?!/?$)",
              "^\\.(?!/?$)",
              "^\\./?$",
              "^.+\\.s?css$"
            ]
          ]
        }
      ],
      "import/no-webpack-loader-syntax": 0
    }
  },
  "jest": {
    "roots": [
      "<rootDir>/test/"
    ],
    "testMatch": [
      "**/__tests__/**/*.{js,jsx,ts,tsx}",
      "**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/test/setupTests.ts"
    ],
    "transform": {
      ".*\\.fixture\\..*": "jest-raw-loader",
      "^.+\\.[jt]sx?$": "react-app-rewired/scripts/utils/babelTransform.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "verbose": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.hstar.paint",
    "extends": null,
    "asar": false,
    "win": {
      "target": "portable"
    },
    "directories": {
      "buildResources": "assets"
    }
  }
}
